
<% include ../parcial/_head %>
<% include ../parcial/_menus %> 
<%if(result[0].R_Uss!=undefined){%>
    <script type="text/javascript">
        const myHeaders=new Headers();
                myHeaders.append('token',` ${localStorage.token}`)
                myHeaders.append('rol',` ${localStorage.rol}`)
                myHeaders.append('_id',` ${localStorage.id}`)
                // console.log(localStorage.token)
                if(localStorage.rol=='administrador'){
                
                    fetch('/token',{
                        headers:myHeaders
                    })
                    fetch('/tokeng',{
                        headers:myHeaders
                    })
                    fetch('/tokenm',{
                        headers:myHeaders
                    })
                }else{window.location.replace('/login')}
    </script>

<div class="container-fluid">
        <div class="row">
            <div class="col-xs-12 lead">
                <ol class="breadcrumb">
                    <li><a href="formE">Nuevo estudiante</a></li>
                    <li><a href="fuserlistE">Listado de estudiantes</a></li>
                </ol>
            </div>
        </div>
    </div>
        <div class="conteiner-fluid" >
            <p class="text-center">
                    <a href="/sendBD"><button type="button" class="btn-lg" style="background-color: rgb(red, green, blue);" > validar</button></a>
            </p> 
        </div>
    <div class="container-fluid">
        <h2 class="text-center all-tittles">Estudiantes del Documento</h2>
        <div class="table-responsive">
            <div class="div-table" style="margin:0 !important;">
                <div class="div-table-row div-table-row-list" style="background-color:#DFF0D8; font-weight:bold;">
                    <div class="div-table-cell" style="width: 6%;">#</div>
                    <div class="div-table-cell" style="width: 15%;">Apellidos</div>
                    <div class="div-table-cell" style="width: 15%;">Nombres</div>
                    <div class="div-table-cell" style="width: 15%;">C.I.</div>
                    <div class="div-table-cell" style="width: 9%;">R.U.</div>
                </div>
            </div>
        </div>


        <% for(var i=0; i < result.length; i++) { %>
            <div class="table-responsive">
                <div class="div-table" style="margin:0 !important;">
                    <div class="div-table-row div-table-row-list">
                            <div class="div-table-cell" style="width: 6%;"><%= i + 1%></div>
                            <div class="div-table-cell" style="width: 15%;"><%= result[i].Paterno +" "+ result[i].Materno %></div>
                            <div class="div-table-cell" style="width: 15%;"><%= result[i].Nombres %></div>
                            <div class="div-table-cell" style="width: 15%;"><%= result[i].C_I %></div> 
                            <div class="div-table-cell" style="width: 9%;"><%= result[i].R_Uss %></div>
                    </div>
                </div>
            </div>
    
        <% } %> 

    <% }else{ %>
       
            <script type="text/javascript">
    
                {alert("Formato del Documento no Valido")}
                window.location.replace('/users')
            </script>
       
    <% } %>
