    <% include ../parcial/_head %>
    <% include ../parcial/_menus %> 
    <script type="text/javascript">
        const myHeaders=new Headers();
                myHeaders.append('token',` ${localStorage.token}`)
                myHeaders.append('rol',` ${localStorage.rol}`)
                myHeaders.append('_id',` ${localStorage.id}`)
                // console.log(localStorage.token)
                if(localStorage.rol=='administrador'){
                
                    fetch('/token',{
                        headers:myHeaders
                    })
                    fetch('/tokeng',{
                        headers:myHeaders
                    })
                    fetch('/tokenm',{
                        headers:myHeaders
                    })
                }else{window.location.replace('/login')}
    </script>
    
    <div class="container-fluid">
           
            <div class="row">
                <div class="col-xs-12 lead">
                    <ol class="breadcrumb">
                        <li><a href="formE">Nuevo estudiante</a></li>
                        <li><a href="fuserlistE">Listado de estudiantes</a></li>
                        <li> <a href="users">Importar</a> </li>
                    </ol>
                </div>
            </div>
        </div>
        <%if(msg1=='exito'){%>
            <h2 class="text-center all-tittles">Datos Verificados Satisfactoriamente </h2><br>
            <!-- <h2 class="text-center all-tittles">Satisfactoriamente </h2> -->
            <form  action="/registrar" method="POST">
                <div class="row">
                    <input type="hidden" name="valor" value="<%=msg1%>">
                    <div class="col-xs-12 col-sm-6 col-sm-offset-2 ">
                        <p class="text-center all-tittles">
                            <!-- <button type="reset" class="btn btn-info" style="margin-right: 20px;"><i class="zmdi zmdi-roller"></i> &nbsp;&nbsp; Limpiar</button> -->
                            <button type="submit" class="btn btn-primary"><i class="zmdi zmdi-floppy"></i><h4>&nbsp;&nbsp;Registrar en la Base de Datos</h4></button>
                        </p> 
                    </div>
            </form>
        <% }else{ %>
            <div class="container-fluid">
                    <h2 class="text-center all-tittles"><%=msg1%></h2>
                    <div class="table-responsive">
                        <div class="div-table" style="margin:0 !important;">
                            <div class="div-table-row div-table-row-list" style="background-color:#DFF0D8; font-weight:bold;">
                                <div class="div-table-cell" style="width: 6%;">#</div>
                                <div class="div-table-cell" style="width: 15%;">Apellidos</div>
                                <div class="div-table-cell" style="width: 15%;">Nombres</div>
                                <div class="div-table-cell" style="width: 15%;">C.I.</div>
                                <div class="div-table-cell" style="width: 9%;">R.U.</div>
                            </div>
                        </div>
                    </div>
            
            
                    <% for(var i=1; i < msg3+1 ; i++) { %>
                        <div class="table-responsive">
                            <div class="div-table" style="margin:0 !important;">
                                <div class="div-table-row div-table-row-list">
                                        <div class="div-table-cell" style="width: 6%;"><%= i %></div>
                                        <div class="div-table-cell" style="width: 15%;"><%= msg2[i].esto.Paterno +" "+ msg2[i].esto.Materno %></div>
                                        <div class="div-table-cell" style="width: 15%;"><%= msg2[i].esto.Nombres %></div>
                                        <div class="div-table-cell" style="width: 15%;"><%= msg2[i].esto.C_I %></div> 
                                        <div class="div-table-cell" style="width: 9%;"><%= msg2[i].esto.R_Uss %></div>
                                </div>
                            </div>
                        </div>
                
                    <% } %> 
            
            
                <p class="text-center">
                    <a href="/users"><button type="button" class="btn btn-danger">Volver a Importar</button></button></a>
                </p> 
            <!-- <script type="text/javascript">
        
                {alert("formato no valido")}
                window.location.replace('/users')
            </script> -->
           
    
        <% } %>
